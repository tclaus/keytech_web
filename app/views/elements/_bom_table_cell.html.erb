
<% if !control.controlType.blank? && control.controlType.downcase == "check" %>
  <% value = listerValueParser(element, control.attributeName) %>
  <% if value == true || value == "true" || value == "1" %>
    <i class="far fa-check-square"></i>
  <% else %>
    <i class="far fa-square"></i>
  <% end %>
<% else %>
  <%= link_to show_element_path(element.elementKey) do %>
    <%= listerValueParser(element, control.attributeName) %>
  <% end %>
<% end %>
